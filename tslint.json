{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "no-any": false,
      "no-boolean-literal-compare": false,
      "no-empty-interface": false,
       // tslint-react
      "jsx-no-multiline-js": false,
      "jsx-no-lambda": false
    },
    "rulesDirectory": []
}
