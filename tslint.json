{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
      "prettier": true,
      "no-any": false,
      "jsx-no-multiline-js": false,
      "jsx-no-lambda": false
    }
}
